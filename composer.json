{
    "name": "brunocfalcao/nidavellir-backend",
    "description": "Nidavellir backend",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/nidavellir-backend",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0",
        "blade-ui-kit/blade-heroicons": "^2.3",
        "blade-ui-kit/blade-icons": "^1.6",
        "codeat3/blade-clarity-icons": "^1.9",
        "codeat3/blade-cryptocurrency-icons": "^1.7",
        "davidhsianturi/blade-bootstrap-icons": "^1.5",
        "postare/blade-mdi": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\Backend\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\Backend\\BackendServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
